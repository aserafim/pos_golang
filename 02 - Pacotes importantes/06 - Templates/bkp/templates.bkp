package main

import (
	"os"
	"text/template"
)

type Curso struct {
	Nome         string
	CargaHoraria int
}

type Cursos []Curso

func main() {
	curso := Curso{"Go", 50}
	t := template.Must(template.New("CursoTemplate").Parse("Curso: {{.Nome}} - Carga Hor√°ria: {{.CargaHoraria}}"))

	err := t.Execute(os.Stdout, curso)
	if err != nil {
		panic(err)
	}

}
